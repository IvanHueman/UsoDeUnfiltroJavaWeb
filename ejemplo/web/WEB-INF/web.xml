<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <filter>
        <filter-name>mideTiempo</filter-name>
        <filter-class>filtros.mideTiempo</filter-class>
    </filter>
    <filter>
        <filter-name>Login</filter-name>
        <filter-class>filtros.Login</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Login</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>mideTiempo</filter-name>
        <servlet-name>Calculadora</servlet-name>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <servlet>
        <servlet-name>Hola</servlet-name>
        <servlet-class>Servlets.Hola</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calculadora</servlet-name>
        <servlet-class>Servlets.Calculadora</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Hola</servlet-name>
        <url-pattern>/Hola</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calculadora</servlet-name>
        <url-pattern>/Calculadora</url-pattern>
    </servlet-mapping>
</web-app>
